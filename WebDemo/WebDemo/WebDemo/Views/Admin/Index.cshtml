@model IEnumerable<WebDemo.Models.PHIM>@{    ViewBag.Title = "AdminPage";    Layout = "~/Views/Shared/_Layout.cshtml";}<h2 class="admin general">ADMIN PAGE</h2><div style="margin: 10px auto; width: 90%;">    <a href="@Url.Action("Create","PHIMs")" class="btn-small" style="margin-bottom:10px;display:inline-block">TẠO MỚI</a>    <a href="@Url.Action("Create","THELOAIs")" class="btn-small" style="margin-bottom:10px;display:inline-block">TẠO MỚI THỂ LOẠI</a>    <a href="@Url.Action("HoaDon","Admin")" class="btn-small" style="margin-bottom:10px;display:inline-block">XEM HÓA ĐƠN</a>    <table class="table">        <tr>            <th>                TÊN SẢN PHẨM            </th>            <th>                MÔ TẢ            </th>            <th>                GIÁ            </th>            <th>                HÌNH            </th>            <th>                THỂ LOẠI            </th>            <th></th>        </tr>        @foreach (var item in Model)        {<tr>    <td>        @Html.DisplayFor(modelItem => item.TenPhim)    </td>    <td>        @Html.DisplayFor(modelItem => item.MoTa)    </td>    <td>        @Html.DisplayFor(modelItem => item.DonGia)    </td>    <td>        <img src="~/Images/@item.HinhMinhHoa" />    </td>    <td>        @Html.DisplayFor(modelItem =>                       item.THELOAI.TenTL)    </td>    <td>        @Html.ActionLink("Sửa", "Edit", "PHIMs", new        {            id =                       item.MaPhim        }, new { @class = "btn-small" })        @Html.ActionLink("Chi tiết", "Details", "PHIMs", new        {            id =                       item.MaPhim        }, new { @class = "btn-small" })        @Html.ActionLink("Xóa", "Delete", "PHIMs", new        {            id =                        item.MaPhim        }, new { @class = "btn-small" })    </td></tr>}    </table></div>