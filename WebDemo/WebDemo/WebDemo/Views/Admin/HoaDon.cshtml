@model IEnumerable<WebDemo.Models.DONDATHANG>@{    ViewBag.Title = "AdminPage";    Layout = "~/Views/Shared/_Layout.cshtml";}<h2 class="admin general">ADMIN PAGE</h2><div style="margin: 10px auto; width: 90%;">    <table class="table">        <tr>            <th>                TÊN NGƯỜI NHẬN            </th>            <th>                NGÀY ĐẶT HÀNG            </th>            <th>                TRỊ GIÁ            </th>            <th>                ĐIỆN THOẠI            </th>            <th>                ĐỊA CHỈ NHẬN            </th>            <th>                CHI TIẾT ĐƠN HÀNG            </th>        </tr>        @foreach (var item in Model)        {<tr>    <td>        @Html.DisplayFor(modelItem => item.TenNguoiNhan)    </td>    <td>        @Html.DisplayFor(modelItem => item.NgayDH)    </td>    <td>        @Html.DisplayFor(modelItem => item.TriGia)    </td>    <td>        @Html.DisplayFor(modelItem => item.DienThoaiNhan)    </td>    <td>        @Html.DisplayFor(modelItem => item.DiaChiNhan)    </td>    <td>        <a href="@Url.Action("CTDonHangID", "Admin", new {id = item.SoDH})">Chi tiết</a>    </td></tr>}    </table>    <div >        <a href="@Url.Action("Index","Admin")" class="btn-small">Trở về</a>    </div></div>