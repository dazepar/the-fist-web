@model IEnumerable<WebDemo.Models.CTDONHANG>@{    ViewBag.Title = "AdminPage";    Layout = "~/Views/Shared/_Layout.cshtml";}<h2 class="admin general">ADMIN PAGE</h2><div style="margin: 10px auto; width: 90%;">    <table class="table">        <tr>            <th>                MÃ ĐƠN HÀNG            </th>            <th>                SỐ LƯỢNG            </th>            <th>                GIÁ            </th>        </tr>        @foreach (var item in Model)        {<tr>    <td>        @Html.DisplayFor(modelItem => item.MaPhim)    </td>    <td>        @Html.DisplayFor(modelItem => item.SoLuong)    </td>    <td>        @Html.DisplayFor(modelItem => item.DonGia)    </td></tr>}    </table>    <div>        <a href="@Url.Action("HoaDon","Admin")" class="btn-small">Trở về</a>    </div></div>